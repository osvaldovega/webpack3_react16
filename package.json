{
  "name": "webpack3_react_base",
  "version": "1.0.0",
  "description": "This is a base project using Webpack 3 and React 16",
  "keywords": [
    "16",
    "react",
    "webpack3"
  ],
  "license": "MIT",
  "author": "Osvaldo Vega Aguero",
  "main": "index.js",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "preinstall": "node tools/nodeVersionCheck.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "open:dist": "babel-node tools/distServer.js",
    "open:src": "babel-node tools/srcServer.js",
    "remove-dist": "rimraf ./dist",
    "setup": "node tools/initialMessage.js && npm install && node tools/endMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\" \"npm run lint:watch\"",
    "start-message": "babel-node tools/startMessage.js",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "invariant": "2.2.2",
    "lodash": "4.17.4",
    "object-assign": "4.1.1",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "warning": "3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.18.13",
    "chalk": "2.3.0",
    "circular-dependency-plugin": "4.2.1",
    "concurrently": "3.5.0",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.0",
    "css-loader": "0.28.7",
    "enzyme": "3.1.1",
    "enzyme-adapter-react-16": "1.0.4",
    "eslint": "4.11.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.1.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "json-loader": "0.5.7",
    "mockdate": "2.0.2",
    "node-sass": "4.6.1",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.0.8",
    "prompt": "1.0.0",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "react-hot-loader": "3.1.2",
    "react-test-renderer": "16.1.0",
    "redux-immutable-state-invariant": "2.1.0",
    "replace": "0.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "2.9.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.20.0",
    "webpack-md5-hash": "0.0.5"
  },
  "engines": {
    "node": ">=6.10.2 <=9.1.0",
    "npm": ">=3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  }
}
